
@{
    ViewBag.Title = "JobAlertDetails List";
}
<!--Main Content Header-->
<div class="main-content-header container-fluid p-4">
    <div>
        <h6 class="main-content-title tx-18 mg-b-5 mg-t-7">JobAlertDetails List</h6>
    </div>
    <div class="btn-list">
        <a id="BtnRefresh" class="btn btn-rounded btn-success"><i class="fe fe-plus-circle mr-2"></i>Refresh</a>
        <a href='/JobAlertDetails/AddEdit?Id=0' class="btn btn-rounded btn-primary"><i class="fe fe-plus-circle mr-2"></i>Add</a>
    </div>
</div>
<div class="container-fluid">
    <div class="row">
        <div class="col-12">
            <div class="card mg-b-20">
                <div class="card-body" style="overflow:auto">
                    <div class="table-responsive">
                        <table id="TblJobAlertDetails" class="table nowrap text-md-nowrap" cellspacing="0">
                            <thead>
                                <tr>
                                    <th>Actions</th>
                                    <th>NameOfJobAlert</th>
                                    <th>Designation</th>
                                    <th>Email</th>
                                    <th>YearOfExperience</th>
                                    <th>JobCategory</th>
                                    <th>WorkExperience</th>
                                    <th>Role</th>
                                    <th>Skills</th>
                                    <th>NoticePeriod</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section scripts {
    <script src="~/Js/JobAlertDetails/index.js"></script>
    <script>
        $(function () {
            JobAlertDetailsIndex.init();
        });
    </script>
}






